{
  "name": "nilelink-subgraph",
  "version": "1.0.0",
  "description": "NileLink Protocol Subgraph for The Graph",
  "scripts": {
    "codegen": "graph codegen",
    "build": "graph build",
    "deploy": "graph deploy --node https://api.thegraph.com/deploy/ --ipfs https://api.thegraph.com/ipfs/",
    "deploy:hosted": "graph deploy --product hosted-service",
    "deploy:studio": "graph deploy --node https://api.studio.thegraph.com/deploy/ --ipfs https://api.thegraph.com/ipfs/",
    "test": "graph test",
    "create-local": "graph create --node http://localhost:8020/",
    "remove-local": "graph remove --node http://localhost:8020/",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001",
    "auth": "graph auth",
    "deploy:polygon": "npm run deploy -- --network polygon"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "^0.98.1",
    "@graphprotocol/graph-ts": "^0.31.0"
  },
  "devDependencies": {
    "matchstick-as": "^0.5.0"
  }
}